    DialogueOffset: {x: 0, y: 0}
    Skippable: 1
    audioClip: {fileID: 0}
    cam: 0
  - fontType: 2
    talkingPerson: 1
    DialogueToPrint: "Ben hi\xE7 bilmiyom ki burdaki \u015Feyleri yeenim. Normal
