    Skippable: 1
    audioClip: {fileID: 0}
    cam: 0
  - fontType: 2
    talkingPerson: 1
    DialogueToPrint: "Sen de saol. Bizim dorun buray\u0131 \xE7oh \xF6vd\xFC yav.
