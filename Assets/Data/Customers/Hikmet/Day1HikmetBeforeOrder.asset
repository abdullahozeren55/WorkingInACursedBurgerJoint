    DialogueOffset: {x: 0, y: 0}
    Skippable: 1
    audioClip: {fileID: 0}
    cam: 0
  - fontType: 1
    talkingPerson: 0
    DialogueToPrint: "\u0130yi yapm\u0131\u015Fs\u0131n amcac\u0131m iyi. Ne vereyim
