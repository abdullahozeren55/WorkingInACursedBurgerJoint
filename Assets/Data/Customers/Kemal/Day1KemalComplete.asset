  dialogueSegments:
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Eline koluna sa\u011Fl\u0131k delikanl\u0131. Borcumuz ne kadar?"
    Skippable: 1
    LettersPerSecond: 32
