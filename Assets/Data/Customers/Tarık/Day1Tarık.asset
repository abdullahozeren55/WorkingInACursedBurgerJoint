  m_Name: "Day1Tar\u0131k"
  m_EditorClassIdentifier: 
  dialogueSegments:
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Kolay gelsin karde\u015Fim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Sa\u011Folas\u0131n abi. Ho\u015Fgeldin."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Ho\u015Fbulduk ho\u015Fbulduk."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Karde\u015Fim bu sabah buraya ya\u015F\u0131 bir adam geldi
      mi? B\xF6yle \u015Fiveli konu\u015Fuyor benden biraz uzunca."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Haa evet evet hat\u0131rlad\u0131m. Geldi abi evet. \"Golay
      gelsin yeenim\" falan diye konu\u015Fuyordu."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: Hah evet o. Ta kendisi.
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: Neden sordun abi?
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "... Kendisi benim babam... Alzheimer hastas\u0131."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "D\xFCn dedim izin g\xFCn\xFCmde seni \u015Furadaki burgerciye
      g\xF6t\xFCreyim. Heveslendi hemen."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Bug\xFCn de akl\u0131ma geldi dedim kesin dayanamay\u0131p
      kendi gitmi\u015Ftir. Do\u011Fru tahmin etmi\u015Fim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Ya\u015Fl\u0131lar\u0131 bilirsin i\u015Fte. \xC7ocuk gibi
      oluyorlar."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Do\u011Fru abi."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Neyse. Bir \u015Fey ald\u0131 m\u0131 peki?"
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Ald\u0131 abi evet. Burger ve i\xE7ecek ald\u0131."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Paras\u0131n\u0131 verdi mi peki?"
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Yok abi. C\xFCzdan\u0131n\u0131 d\xFC\u015F\xFCrm\xFC\u015F
      ben de \"Sonra verirsin amca.\" dedim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Tahmin etti\u011Fim gibi. C\xFCzdan\u0131n\u0131 d\xFC\u015F\xFCrmemi\u015Ftir.
      Hi\xE7 yan\u0131na alm\u0131yor ki. Anahtarlar\u0131n\u0131 almay\u0131 bile
      nadiren hat\u0131rl\u0131yor."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Anlad\u0131m... \u0130yi bari c\xFCzdan\u0131n\u0131n kaybolmamas\u0131na
      sevindim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Kay\u0131p c\xFCzdan... Ke\u015Fke tek derdimiz o olsa."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Neyse. Kendi dertlerimle kafan\u0131 \u015Fi\u015Firmeyim.
      Borcu ne kadard\u0131 babam\u0131n?"
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "... Bir tane Klasik Burger alm\u0131\u015Ft\u0131. O 300 TL...
      Bir de Sar\u0131 Kula alm\u0131\u015Ft\u0131. O da 50TL. Toplam 350TL."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Buyur karde\u015Fim \u015F\xF6yle vereyim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Olur da tekrar gelirse sana zahmet ne isterse yine verir misin?
      Ben ak\u015Fam i\u015F \xE7\u0131k\u0131\u015F\u0131 u\u011Frar \xF6derim ne
      ald\u0131ysa."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: Tabii abi.
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "\xC7ok sa\u011Fol karde\u015Fim. Ben Tar\u0131k bu arada. Babam
      da Hikmet."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Memnun oldum Tar\u0131k abi. Ben de Sinan."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: TARIK
    DialogueToPrint: "Ben de memnun oldum karde\u015Fim. Hadi dikkat et kendine."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
