  dialogueSegments:
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Kolay gelsin karde\u015Fim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Sa\u011Folas\u0131n abi. Ho\u015Fgeldin."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Ho\u015Fbulduk ho\u015Fbulduk."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Karde\u015Fim bu sabah buraya ya\u015F\u0131 bir adam geldi
      mi? B\xF6yle \u015Fiveli konu\u015Fuyor benden biraz uzunca."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Haa evet evet hat\u0131rlad\u0131m. Geldi abi evet. \"Golay
      gelsin yeenim\" falan diye konu\u015Fuyordu."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: Hah evet o. Ta kendisi.
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: Neden sordun abi?
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "... Kendisi benim babam... Alzheimer hastas\u0131."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "D\xFCn dedim izin g\xFCn\xFCmde seni \u015Furadaki burgerciye
      g\xF6t\xFCreyim. Heveslendi hemen."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Bug\xFCn de akl\u0131ma geldi dedim kesin dayanamay\u0131p
      kendi gitmi\u015Ftir. Do\u011Fru tahmin etmi\u015Fim."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Ya\u015Fl\u0131lar\u0131 bilirsin i\u015Fte. \xC7ocuk gibi
      oluyorlar."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: BEN
    DialogueToPrint: "Do\u011Fru abi."
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: ...
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  - PersonName: "M\xDC\u015ETER\u0130"
    DialogueToPrint: "Neyse. Bir \u015Fey ald\u0131 m\u0131 peki?"
    Skippable: 1
    LettersPerSecond: 32
    audioClip: {fileID: 0}
  type: 1
  question: "Paras\u0131 olmad\u0131\u011F\u0131 i\xE7in bir \u015Fey vermedi\u011Fimi
    itiraf etmeli miyim? \xC7ok agresif birine benziyor."
  optionA: "DO\u011ERUYU S\xD6YLE"
  optionD: "YALAN S\xD6YLE"
